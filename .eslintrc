{
   "parser": "babel-eslint",
   "env": {
      "es6": true,
      "browser": true,
      "node": true
   },
   "rules": {
      "max-len": "off",
      "spaced-comment": "off",
      "linebreak-style": "off",
      "object-curly-spacing": "off",
      "indent": "off",
      "require-jsdoc": "off",
      "operator-linebreak": "off",
      "quotes": "off",
   },
   "extends": [
      "eslint:recommended",
      "google"
   ]
}